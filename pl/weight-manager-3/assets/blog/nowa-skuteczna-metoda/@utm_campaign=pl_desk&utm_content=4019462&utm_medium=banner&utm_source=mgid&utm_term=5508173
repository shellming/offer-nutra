    wpush.gaev = function(eventLabel, eventValue){
        ga && ga('send', 'event', 'webpush', 'click', eventLabel, eventValue || 1)
    }
    
    wpush.init = function(initData){
    	
      var chrome   = navigator.userAgent.indexOf('Chrome') > -1;
    	var firefox  = navigator.userAgent.indexOf('Firefox') > -1;
    	var edge  = navigator.userAgent.indexOf('Edge') > -1;
    	
      
      if(!chrome && !firefox && !edge){
        this.gaev('wrong_browser')
      	return;
      }
      
      
      this.inject_css()
      this.inject_html()
    
    	this._data = initData || {}
      
    	if(!Cookies.get('wpush-agreed')){
      	this.gc().style.display="block"
      }
      
      if(this._dev){
      	this.gc().style.display="block"
      }
      
      this.gaev('init')
    }
    
    wpush.open = function(url) {
    	if(!this._dev){
      	Cookies.set('wpush-agreed','1', { expires: 30*120 })
      }
      
      var params = "?t="+ encodeURIComponent(JSON.stringify(this._data));
      
      if(this._ty1){
          params += "&ty1="+encodeURIComponent(this._ty1);
      }
      if(this._ty2){
          params += "&ty2="+encodeURIComponent(this._ty2);
      }
      
      this.gaev('open')
    	var url = url || this._url + params
    	var width = 600
      var height = 550
    	var left = (window.innerWidth / 2) - (width / 2)
      var top = (window.innerHeight / 2) - (height / 2)
      var win = window.open(url, '_blank', "height="+height+",width="+width+",left="+left+",top="+top+"");
      win.focus();
      
      this.close()
    }
    
    wpush.close = function(manual){
        if(manual){
            this.gaev('close')    
        }
        
      this.gc().style.display = 'none';
    }
    
    window.wpush = wpush
    
    window.__onDomReady888__(function(){
        window.wpush.gaev('page_load')
        if(window.wpushinit || window.wpushinit === undefined){
            window.wpush.init(__vl['wpd'])
        }
    })

